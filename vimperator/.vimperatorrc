" vim:fdm=marker:foldlevel=0
" Source vimperatorrc
command! sourcerc :source ~/.vimperatorrc

" Set duckduckgo as default search
set defsearch=duckduckgo

" Ctrl-a
inoremap <C-a> <C-Home><C-S-End>

" Disable bell
javascript liberator.beep = function() { return false; }

" Pass through keys
set passthrough=true

" Changing tabs {{{
nnoremap < gT
nnoremap > gt
nnoremap <S-h> gT
nnoremap <S-j> gt
nnoremap <S-k> gT
nnoremap <S-l> gt
" }}}

" Custom commands
command! wetter :tabopen https://www.blogs.uni-mainz.de/fb08-ipa/wetter/
command! mensa :tabopen http://www.studierendenwerk-mainz.de/essentrinken/speiseplan/?displayall=1
